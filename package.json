{
  "name": "vscode-svn-ai",
  "displayName": "VSCode SVN",
  "description": "继承了svn基本功能,并增加了AI提交日志功能",
  "version": "4.3.0",
  "publisher": "pengfeiSummer",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "SCM Providers",
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-svn.uploadFile",
    "onCommand:vscode-svn.uploadFolder",
    "onCommand:vscode-svn.commitFile",
    "onCommand:vscode-svn.revertFile",
    "onCommand:vscode-svn.setSvnRoot",
    "onCommand:vscode-svn.clearSvnRoot",
    "onCommand:vscode-svn.updateFile",
    "onCommand:vscode-svn.updateDirectory",
    "onCommand:vscode-svn.updateWorkspace",
    "onCommand:vscode-svn.viewLog",
    "onCommand:vscode-svn.showLocalRevision",
    "workspaceContains:.svn"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-svn.uploadFile",
        "title": "SVN: 上传文件"
      },
      {
        "command": "vscode-svn.revertFile",
        "title": "SVN: 恢复文件"
      },
      {
        "command": "vscode-svn.uploadFolder",
        "title": "SVN: 上传文件夹"
      },
      {
        "command": "vscode-svn.commitFile",
        "title": "SVN: 提交文件(显示差异)"
      },
      {
        "command": "vscode-svn.setSvnRoot",
        "title": "SVN: 设置SVN工作副本路径"
      },
      {
        "command": "vscode-svn.clearSvnRoot",
        "title": "SVN: 清除SVN工作副本路径"
      },
      {
        "command": "vscode-svn.updateFile",
        "title": "SVN: 更新文件"
      },
      {
        "command": "vscode-svn.updateDirectory",
        "title": "SVN: 更新目录"
      },
      {
        "command": "vscode-svn.updateWorkspace",
        "title": "SVN: 更新工作区"
      },
      {
        "command": "vscode-svn.viewLog",
        "title": "SVN: 查看日志记录"
      },
      {
        "command": "vscode-svn.showLocalRevision",
        "title": "SVN: 显示本地修订版本号"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-svn.showLocalRevision",
          "when": "resourceScheme == file",
          "group": "2_svn@1"
        },
        {
          "command": "vscode-svn.uploadFile",
          "when": "resourceScheme == file && !explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.revertFile",
          "when": "resourceScheme == file && !explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.uploadFolder",
          "when": "explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.commitFile",
          "when": "resourceScheme == file && !explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.updateFile",
          "when": "resourceScheme == file && !explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.updateDirectory",
          "when": "explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.viewLog",
          "when": "resourceScheme == file",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.setSvnRoot",
          "when": "explorerResourceIsFolder",
          "group": "2_svn@9"
        }
      ],
      "editor/context": [
        {
          "command": "vscode-svn.showLocalRevision",
          "when": "resourceScheme == file",
          "group": "2_svn@1"
        },
        {
          "command": "vscode-svn.commitFile",
          "when": "resourceScheme == file",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.revertFile",
          "when": "resourceScheme == file",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.updateFile",
          "when": "resourceScheme == file",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.viewLog",
          "when": "resourceScheme == file",
          "group": "2_svn"
        }
      ],
      "commandPalette": [
        {
          "command": "vscode-svn.setSvnRoot",
          "group": "svn"
        },
        {
          "command": "vscode-svn.clearSvnRoot",
          "group": "svn"
        },
        {
          "command": "vscode-svn.updateWorkspace",
          "group": "svn"
        },
        {
          "command": "vscode-svn.showLocalRevision",
          "group": "svn"
        }
      ],
      "view/title": [
        {
          "command": "vscode-svn.updateWorkspace",
          "when": "view == explorer",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "VSCode SVN",
      "properties": {
        "vscode-svn.customSvnRoot": {
          "type": "string",
          "default": null,
          "description": "自定义SVN工作副本根目录路径"
        },
        "vscode-svn.aiModel": {
          "type": "string",
          "enum": [
            "openai",
            "qwen"
          ],
          "default": "openai",
          "description": "选择用于生成提交日志的AI模型（OpenAI 或 通义千问）"
        },
        "vscode-svn.aiApiKey": {
          "type": "string",
          "default": "",
          "description": "用于生成提交日志的OpenAI API密钥"
        },
        "vscode-svn.qwenApiKey": {
          "type": "string",
          "default": "",
          "description": "用于生成提交日志的通义千问API密钥"
        },
        "vscode-svn.showUncommittedChanges": {
          "type": "boolean",
          "default": true,
          "description": "在日志列表中显示未提交到本地的修改"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/summer198971/vscode-svn.git"
  },
  "bugs": {
    "url": "https://github.com/summer198971/vscode-svn/issues"
  },
  "homepage": "https://github.com/summer198971/vscode-svn",
  "keywords": [
    "svn",
    "subversion",
    "vscode",
    "scm",
    "版本控制",
    "差异对比",
    "AI提交日志",
    "本地版本对比"
  ],
  "author": "summer198971",
  "license": "MIT",
  "dependencies": {
    "@types/xmldom": "^0.1.34",
    "@xmldom/xmldom": "^0.9.8"
  }
}
